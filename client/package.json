{
  "name": "arc-hive-client",
  "version": "0.8.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "case-formatter": "^1.0.9",
    "html-react-parser": "^0.10.0",
    "query-string": "^6.9.0",
    "react": "^16.11.0",
    "react-aad-msal": "^1.1.2",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-image": "^2.2.2",
    "react-loader-spinner": "^3.1.5",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-syntax-highlighter": "^12.2.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "string": "^3.3.3",
    "style-to-object": "^0.3.0",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "start": "PORT=8000 react-scripts start",
    "build-prod": "react-scripts build",
    "build-dev": "dotenv -e .env.development react-scripts build",
    "deploy-prod": "aws s3 sync --cache-control 'max-age=604800' --exclude index.html build/ s3://arc-hive-front && aws s3 sync --cache-control 'no-cache' build/ s3://arc-hive-front",
    "deploy-dev": "aws s3 sync build/ s3://arc-hive-front-dev",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-emotion": "^10.0.27",
    "dotenv-cli": "^3.1.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  }
}
